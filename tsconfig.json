{
  "compilerOptions": {
    "target": "es2016",
    "module": "es2020",
    "lib": [
      "es2021",
      "dom"
    ],
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "vite/client",
      "node"
    ],
    "jsx": "react",
    "resolveJsonModule": true
  }
}
